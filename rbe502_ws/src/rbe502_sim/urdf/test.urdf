<?xml version="1.0"?>
<robot name="bot">
  <link name="base_link"> <!-- top_center_plate-->
    <visual>
      <geometry>
        <mesh filename="package://rbe502_sim/meshes/BalanceRobot/CenterPlates.stl"/>
      </geometry>
      
    </visual>
  </link>

  <link name="mid_plate"> <!-- top_center_plate-->
    <visual>
      <geometry>
        <mesh filename="package://rbe502_sim/meshes/BalanceRobot/CenterPlates.stl"/>
      </geometry>
      <origin xyz="0 -150.66 0"/>
    </visual>
  </link>

  <link name="left_side_wall">
    <visual>
      <geometry>
        <mesh filename="package://rbe502_sim/meshes/BalanceRobot/SideWall.stl"/>
      </geometry>
      <origin xyz="0 -146.44 58.6"/>
    </visual>
  </link>

  <link name="right_side_wall">
    <visual>
      <geometry>
        <mesh filename="package://rbe502_sim/meshes/BalanceRobot/SideWall.stl"/>
      </geometry>
      <origin xyz="0 -146.44 -58.6"/>
    </visual>
  </link>

   <link name="left_motor_mount">
     <visual>
       <geometry>
         <mesh filename="package://rbe502_sim/meshes/BalanceRobot/MotorMount.stl"/>
       </geometry>
       <origin xyz="0 -300 30.8"/>
     </visual>
   </link>

   <link name="right_motor_mount">
     <visual>
       <geometry>
         <mesh filename="package://rbe502_sim/meshes/BalanceRobot/MotorMount.stl"/>
       </geometry>
       <origin xyz="0 -300 -30.8" rpy="0 3.1415 0"/>
     </visual>
   </link>

  <link name="left_motor">
    <visual>
      <geometry>
        <mesh filename="package://rbe502_sim/meshes/BalanceRobot/Stepper.stl"/>
      </geometry>
      ,<origin xyz="0 -300 29"/>
    </visual>
  </link>

  <link name="right_motor">
    <visual>
      <geometry>
        <mesh filename="package://rbe502_sim/meshes/BalanceRobot/Stepper.stl"/>
      </geometry>
      <origin xyz="0 -300 -33" rpy="0 0 0"/>
    </visual>
  </link>


  <link name="left_wheel">
    <visual>
      <geometry>
        <mesh filename="package://rbe502_sim/meshes/BalanceRobot/Wheel.stl"/>
      </geometry>
      <origin xyz="0 -300 75"/>
    </visual>
  </link>

  <link name="right_wheel">
    <visual>
      <geometry>
        <mesh filename="package://rbe502_sim/meshes/BalanceRobot/Wheel.stl"/>
      </geometry>
      <origin xyz="0 -300 -75" rpy="0 3.1415 0"/>
    </visual>
  </link>

  



  <joint name="base_to_left_wall" type="fixed">
    <parent link="base_link"/>
    <child link="left_side_wall"/>
  </joint>

  <joint name="base_to_right_wall" type="fixed">
    <parent link="base_link"/>
    <child link="right_side_wall"/>
  </joint>

  <joint name="base_to_left_motor_mount" type="fixed">
    <parent link="left_side_wall"/>
    <child link="left_motor_mount"/>
  </joint>

  <joint name="base_to_right_motor_mount" type="fixed">
    <parent link="right_side_wall"/>
    <child link="right_motor_mount"/>
  </joint>

  <joint name="base_to_left_motor" type="fixed">
    <parent link="base_link"/>
    <child link="left_motor"/>
  </joint>

  <joint name="base_to_right_motor" type="fixed">
    <parent link="base_link"/>
    <child link="right_motor"/>
  </joint>

  <joint name="base_to_left_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="left_wheel"/>
  </joint>

  <joint name="base_to_right_wheel" type="fixed">
    <parent link="base_link"/>
    <child link="right_wheel"/>
  </joint>

  <joint name="base_to_mid_plate" type="fixed">
    <parent link="base_link"/>
    <child link="mid_plate"/>
  </joint>



